#include <stdio.h>
//#define _CRT_SECURE_NO_WARNINGS

int main(void) {
	int age;
	char name[11];//+1 , 한글5자
	//printf("몇살? ");
	//scanf_s("%d", &age); //입력 함수전 뭘 입력할지 출력하기 문자가 앞에 붙은 변수 뒤에는 &를 사용
	//printf("이름이 뭐니? ");
	//scanf_s("%s", name, 11);
	//문자를 입력받을땐 문자변수 뒤에 문자개수를 반드시 포함
	printf("이름과 나이입력: ");
	scanf_s("%s %d", name, 11, &age);

	printf("너의 이름은 %s이고, 나이는 %d살이구나~\n", name, age);
	return 0;
}
